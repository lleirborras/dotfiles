[alias]
  pus = push
  pul = pull
  st = status
  ci = commit
  co = checkout
  br = branch
  up = !git fetch origin && git rebase origin/master
  out = log origin..HEAD
  l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
  ui = auto
[format]
  pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
